{
  "build": {
    "distDir": "../public",
    "devPath": "http://127.0.0.1:6969",
    "beforeDevCommand": "kill -9 $(lsof -i :3003 -t) &>/dev/null || true && http-server -p 6969",
    "beforeBuildCommand": "kill -9 $(lsof -i :3003 -t) &>/dev/null || true && http-server -p 6969"
  },
  "ctx": {},
  "tauri": {
    "embeddedServer": {
      "active": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "net.hackcraft.blob-shooter",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "osx": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false
      },
      "exceptionDomain": ""
    },
    "allowlist": {
      "all": true
    },
    "window": {
      "title": "Blob Shooter",
      "resizable": false,
      "fullscreen": true
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    },
    "inliner": {
      "active": true
    }
  }
}